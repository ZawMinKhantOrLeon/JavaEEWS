    <%! 
    int max(int one,int two,int three,int four){
		int max=one;
		
		if(two>max)
			max=two;
		
		if(three>max)
			max=three;
		
		if(four>max)
			max=four;
		
		 return max;
     }
    
    int min(int one,int two,int three,int four){
		int min=one;
		
		if(two<max)
			max=two;
		
		if(three<max)
			max=three;
		
		if(four<max)
			max=four;
		
		 return max;
     }
    %>
