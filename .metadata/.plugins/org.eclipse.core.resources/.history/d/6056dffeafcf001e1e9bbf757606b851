    pageEncoding="ISO-8859-1"%>
<%@ taglib uri="jakarta.tags.core" prefix="c" %>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Jstl Core Var</title>
</head>
<body>
	<c:set var="salary"  value="300000000000"/>
	<c:set var="age"  value="21"/>
	
	<p>salary : ${salary }</p>
	<p>age: <c:out value="${age }" /> </p>
</body>
</html>