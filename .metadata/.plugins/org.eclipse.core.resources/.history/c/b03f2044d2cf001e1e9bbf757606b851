	<c:set name="password" value="apple"></c:set>
	
	<c:url var="dynamicLink" value="session-data.jsp">
		<c:param name="name" value="${name }"></c:param>
		<c:param name="password" value="${password }"></c:param>
	</c:url>
	
	<a href="${dynamicLink }">Dynamic Link</a>
	
</body>
